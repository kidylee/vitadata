<p id="notice"><%= notice %></p>

<h1>Encriptions</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Value</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @encriptions.each do |encription| %>
      <tr>
        <td><%= encription.name %></td>
        <td><%= encription.value %></td>
        <td><%= encription.user %></td>
        <td><%= link_to 'Show', encription %></td>
        <td><%= link_to 'Edit', edit_encription_path(encription) %></td>
        <td><%= link_to 'Destroy', encription, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Encription', new_encription_path %>
