<p id="notice"><%= notice %></p>

<h1>Txns</h1>

<table>
  <thead>
    <tr>
      <th>Txnid</th>
      <th>From</th>
      <th>Block</th>
      <th>Label</th>
      <th>Address</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @txns.each do |txn| %>
      <tr>
        <td><%= txn.txnid %></td>
        <td><%= txn.from %></td>
        <td><%= txn.block %></td>
        <td><%= txn.label %></td>
        <td><%= txn.address %></td>
        <td><%= link_to 'Show', txn %></td>
        <td><%= link_to 'Edit', edit_txn_path(txn) %></td>
        <td><%= link_to 'Destroy', txn, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Txn', new_txn_path %>
